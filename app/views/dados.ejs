<!-- FLOT RESIZE PLUGIN - allows the chart to redraw when the window is resized -->
<script src="fontawesome-free-5.12.0-web/plugins/flot-old/jquery.flot.resize.min.js"></script>
<!-- FLOT PIE PLUGIN - also used to draw donut charts -->
<script src="fontawesome-free-5.12.0-web/plugins/flot-old/jquery.flot.pie.min.js"></script>
<!-- Page script -->
<script>  

  $(function () {
    
     /*
     * DONUT CHART
     * -----------
     */  
     var donutData = [
      {
        label: 'Homens',
        data :  20,
        color: '#3c8dbc'
      },
      {
        label: 'Series3',
        data : 50,
        color: '#0073b7'
      }
    ]
    $.plot('#donut-chart', donutData, {
      series: {
        pie: {
          show       : true,
          radius     : 1,
          innerRadius: 0.5,
          label      : {
            show     : true,
            radius   : 2 / 3,
            formatter: labelFormatter,
            threshold: 0.1
          }

        }
      },
      legend: {
        show: false
      }
    })
    /*
     * END DONUT CHART
     */

  })

  /*
   * Custom Label formatter
   * ----------------------
   */
  function labelFormatter(label, series) {
    return '<div style="font-size:13px; text-align:center; padding:2px; color: #fff; font-weight: 600;">'
      + label
      + '<br>'
      + Math.round(series.percent) + '%</div>'
  }
</script>